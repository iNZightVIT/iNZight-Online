gwps.pw.bubble.plot = function(gs.num.model.genes,gs.num.sig.SNPs,gs.num.genes,gs.labels,
inches=0.35,fg="grey",cex=0.6,bg="heat", # bg can also be gray
xlab="Number of genes in Regression model",
ylab="Number of significant SNPs in Gene set",
...){
order.indexes = order(gs.num.genes,decreasing=T)
gs.num.genes = gs.num.genes[order.indexes]
gs.num.model.genes = gs.num.model.genes[order.indexes]
gs.num.sig.SNPs = gs.num.sig.SNPs[order.indexes]
gs.labels = gs.labels[order.indexes]
radius <- sqrt(gs.num.genes/pi)
colors = heat.colors(length(radius)+10)
colors = colors[1:(length(colors)-10)]
if(bg == "gray"){
colors = gray(20:(length(radius)+20)/(length(radius)+40))
}
# layout matrix
LO <- matrix(c(1,2), nrow=1, ncol=2, byrow=TRUE)
#Resolution, pointsize
RESO <- 400
PS <- 10
#Overall units in inches
WIDTHS <- c(5,1) #widths of each figure in layout (i.e. column widths)
HEIGHTS <- c(3.5)  #heights of each figure in layout (i.e. row heights)
#Outer margins and calculation of full dimensions
OMA <- c(0,0,1,0)  #Outer margins c(bottom, left, top, right)
HEIGHT <- sum(HEIGHTS) + OMA[1]*PS*1/72 + OMA[3]*PS*1/72
WIDTH <- sum(WIDTHS) + OMA[2]*PS*1/72 + OMA[4]*PS*1/72
par(oma=OMA, ps=PS) #settings before layout
layout(LO, heights=HEIGHTS, widths=WIDTHS)
#layout.show(max(LO)) # run to see layout; comment out to prevent plotting during .pdf
par(cex=1) # layout has the tendency change par()$cex, so this step is important for control
# plot 1
symbols(gs.num.sig.SNPs, gs.num.model.genes, circles=radius, inches=inches,
fg="lightgrey", bg=colors, xlab=xlab,ylab=ylab,...)
text(gs.num.sig.SNPs, gs.num.model.genes, gs.labels, cex=cex)
# Color legend
mar.save = par()$mar
par(mar=c(par()$mar[1],0,par()$mar[3],4)) # set margins
plot.new() # new plot
first.height = 1/length(colors)
height = first.height
first.pos = 0
for(c in 1:length(colors)){
color.indexes=length(colors):1
rect(0,first.pos,1,height,col=colors[color.indexes[c]],border=NA)
first.pos = first.height*c
height = first.pos+first.height
}
